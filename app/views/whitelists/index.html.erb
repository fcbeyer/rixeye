<h1>Listing whitelists</h1>

<table>
  <tr>
    <th>Branch Name</th>
    <th>Project ID</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @whitelists.each do |whitelist| %>
  <tr>
    <td><%= whitelist.branch_name %></td>
    <td><%= whitelist.project_id %></td>
    <td><%= link_to 'Show', whitelist %></td>
    <td><%= link_to 'Edit', edit_whitelist_path(whitelist) %></td>
    <td><%= link_to 'Delete', whitelist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Whitelist', new_whitelist_path %>
